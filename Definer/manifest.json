{
  "manifest_version": 2,
  "name": "Definer",
  "version": "0.02",
  "content_scripts": [
    {
      "matches":  [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ],
  "permissions": [
    "tabs"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "assets/favicon.png",
    "default_popup": "sketch/index.html",
    "default_title": "A popup will come here."
  }
}
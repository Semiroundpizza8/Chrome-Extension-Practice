{
  "manifest_version": 2,
  "name": "Chrome Extension Practice",
  "version": "0.02",
  "content_scripts": [
    {
      "matches":  [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "assets/favicon.png"
  }
}